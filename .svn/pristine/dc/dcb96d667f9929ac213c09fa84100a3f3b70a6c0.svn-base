/* correlation.css start 分层管理-相关性管理  */
.correlation{ height:100px; border-top:6px solid #f0f3f7; padding-left:22px;}
.correlation_title{ height:54px; line-height:54px; font-weight:bold;}
.correlation_formula input{ width:48px; height:26px; border-radius:3px; text-align:center; border:1px solid #c2c8d0; color:#38abf8;}
.correlation_formula i,.correlation_formula span{ font-size:12px; clear:#7b8da0; line-height:28px;}
.correlation_formula strong{ font-size:12px; font-weight:bold;line-height:28px;}
.correlation_formula .active{ border:1px solid #38abf8;}
.correlation_box{border-top:6px solid #f0f3f7; padding:0 22px;}
.correlation_content,.table_pos,.table_bottom_box,.correlation_box{position:relative;}
.correlation_l{ position:relative; z-index:1; width:126px; height:414px; overflow:hidden;}
.correlation_module th,.correlation_module td{ font-weight:bold;}
.correlation_module td{font-size:12px; color:#4b5e70; text-align:left; text-indent:24px;}
.correlation_pos{ position:absolute;left:0;top:0;z-index:1;}
.correlation_pos1{ position:relative; z-index:5;}
.correlation_pos1 th{ height:41px; line-height:41px;}
.correlation_pos2{ position:absolute;left:0;top:0; z-index:4; width:inherit; }
.correlation_pos2 th{ background:#fff; width:120px; min-width:120px;border-left:none;}
.correlation_r{ position:absolute;left:126px;top:0; z-index:1;}
.table_title{ width:100%; overflow:hidden;height:44px;}
.correlation_pos3{position:absolute;left:0;top:0;z-index:3;width:inherit;}
.correlation_pos3 td{padding:0;width:120px; min-width:120px; border-top:none; border-left:none;}
.table_bottom_box{ height:371px; overflow:hidden;}
.scroll_box1{ height:371px; background:#f0f3f7; position:absolute;right:0px; top:97px; width:4px; overflow:hidden;border-radius:2px;}
.scroll_son1{position:absolute;right:0px; top:0px;width:4px; border-radius:2px; z-index:1; background:#38abf8; height:60px; cursor:pointer;}
.table_left_box{ height:371px;}
.checkInfo_h3{ font-size:16px; color:#7b8da0; font-weight:bold; padding-bottom:20px;}
.tip_box{ width:418px; height:48px; line-height:48px; border-radius:5px; background:#f0f5f7; border:1px solid #dee1e2; font-size:12px;text-align:center; margin-left:22px;}
.correlation_pos3 input{ text-align:center; width:100%; height:100%; color:#7b8da0;}
/* correlation.css end 分层管理-相关性管理  */

