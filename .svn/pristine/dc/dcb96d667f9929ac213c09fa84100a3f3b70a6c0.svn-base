/* 新增报表模板 */
.updataRate_box{
	width: 460px;
	height: auto;
}
.updataRate_list li{
	overflow: visible;
}
.updataRate_list li span{
	width: 100px;
	font-weight: bold;
}
.updataRate_list li div, .updata_file{
	width: 290px;
}
.updataRate_box .main_btn1{
	margin-top: 20px;
}
.download_example{
	display: block;
	line-height: 14px;
	height: 14px;
	margin-left: 100px;
	color: #38abf8;
	font-size: 0;
	cursor: pointer;
}
.download_example span{
	font-size: 12px;
	vertical-align: middle;
}
.download_example b{
	margin-left: 5px;
	display: inline-block;
	width: 10px;
	height: 10px;
	background: url(image/download_example.png) no-repeat;
	vertical-align: middle;
}
.report_select{
	position: relative;
	cursor: pointer;
}
.report_select_icon{
	position: absolute;
	right: 10px;
	top: 0;
	display: inline-block;
	width: 14px;
	height: 26px;
	cursor: pointer;
	z-index: 99;
}
.report_select_icon i{
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -2.5px;
	margin-left: -4px;
	display: inline-block;
	width: 8px;
	height: 5px;
	background: url(../image/triangle1.png) no-repeat;
	z-index: 99;
}
.report_select_icon .act{
	background: url(../image/triangle2.png) no-repeat;

}
.report_options{
	display: none;
	position: absolute;
	top: 30px;
	left: 0;
	width: 100%;
	line-height: 26px;
	font-size: 14px;
	color: #4b5e70;
	text-align: left;
	background: #fff;
	z-index: 100;
	border-radius: 3px;
	border: 1px solid #ddd;
}
.report_options li{
	padding-left: 10px;
	margin-bottom: 0;
	max-height: 130px;
	cursor: pointer;
}
.report_options li:hover{
	background: #ddd;
}


