/*-- table-drag start --*/
table{border-collapse:collapse;border-spacing:0;}
.table-drag-item{width: 30%; float: left;}
.table-drag-tit{text-align:center;font-size:14px; line-height:36px; color:#fff; position:relative;}
.table-drag-con{width: 100%;word-break:break-all;word-wrap:break-word;}
.table-drag-con td,.table-drag-con th{padding-bottom:20px;font-size: 12px; line-height: 18px;}
.table-drag-con td{height: 32px;}
.table-drag-col-1{width: 40px;}
.table-drag-col-3{width: 50px;}
.table-drag-col-4{width: 80px;}
.table-drag-con thead th{background: #f1f1f1; color: #222; font-weight: normal;}
.table-drag-tags{ margin-bottom:10px; display:block;}


.table-drag-con .table-drag-tags-box{border: 1px dashed #e5e9f4;min-height:60px; padding:20px 20px; border-radius:5px; position: relative;}
.table-drag-tags i{font-style: normal;background:#e5e9f4;height: 36px; line-height: 36px; overflow: hidden; padding:0 8px; font-size: 12px; display:block; border-radius:3px;}
.table-drag-tags em,.table-drag-tags-mirror em{font-style: normal; width: 50%; float: left; text-align: center;}
.table-drag-list-level,.table-drag-checkbox-td{text-align: center;}
.table-drag-tags.active{opacity: 0.5;}
.table-drag-tags-mirror{position: absolute; display:block;}
.table-drag-tags-mirror i{font-style: normal;background:#C4CDE6;height: 36px; line-height: 36px; overflow: hidden; padding:0 8px; font-size: 12px; display:block;}
.table-drag-add,.table-drag-del{width: 20px; height: 20px; margin: 0 2px; display: inline-block;}
.table-drag-add{display: none; background:url(../image/add.png) no-repeat;}
.table-drag-del{display: none; background:url(../image/del.png) no-repeat;}
.table-drag-tags-box{margin-right: 16px;}
.table-drag-con-default{height: 100%;}

.table-drag-tags-box.active{background:rgba(47,266,191,0.1);}


.table-drag-item{width: 46%; margin: 0 2%; float: left;}
.table-drag-tit,.table-drag-tit-default{ height:36px; background:#7ba3ff; border-radius:3px;}
.table-drag-tit-default span{ width:50%; line-height:36px; float:left; height:36px; color:#fff; text-align:center;}
.drap_right_box span{ width:100%;}
.table-drag-tags-box-main{overflow: hidden;}
.drag-item-box{height: 618px;margin-top:14px; position:relative; overflow-y: auto;}
.table-drag-tags-box-tips{color: #c3c3c3;text-align: center; line-height: 60px;}

.table-drag-tags-box-more{width: 9px; height: 7px; position: absolute; left: 50%; bottom: 12px;background: url(../image/drap_btn2.png) no-repeat; cursor: pointer;}
.table-drag-tags-box-more.hide1{background: url(../image/drap_btn1.png) no-repeat;}
/*-- table-drag end --*/


