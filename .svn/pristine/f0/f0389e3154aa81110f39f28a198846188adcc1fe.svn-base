@charset "utf-8";
@import url(reset.css);
/* Global */
body { -moz-user-select: none; }
/* Float */
/*.clearfix:after {
    visibility: hidden;
    display: block;
    clear: both;
    height: 0;
    font-size: 0;
    content: " ";
}
.clearfix {
    zoom: 1;
}*/

.clearfix:after {
    content:"\200B";
    display:block;
    height:0;
    clear:both;
}
.clearfix {*zoom:1;}/*IE/7/6*/
.fl{
    float: left;
}
.fr{
    float: right;
}
/* End float */

/* Components */

/* Form element */

.j-check{
    display: inline-block;
    font-size: 0;
}
.j-check i{
    display: inline-block;
    width: 16px;
    height: 16px;
    margin-right: 10px;
    vertical-align: middle;
    background: url(../img/j_unchecked.png) no-repeat;
    cursor: pointer;
}
.j-check i.checked{
    background: url(../img/j_checked.png) no-repeat;
}
.j-check b{
    display: inline-block;
    font-size: 14px;
    color: #fff;
    vertical-align: middle;
}

.j-label{
    display: inline-block;
    height: 36px;
    font-weight: normal;
    font-size: 14px;
    line-height: 36px;
    color: #fff;
}
.j-input{
    padding: 0 25px 0 15px;
    height: 34px;
    font-size: 14px;
    line-height: 34px;
    color: #4fa0ef;
    background-color: rgba(6, 34, 71,0.5);
    border:1px solid rgb(55, 91, 148);
    border-radius: 4px;
}
.j-button{
    display: inline-block;
    width: 82px;
    height: 34px;
    line-height: 34px;
    background: rgba(25, 145, 235,.5);
    border: 1px solid rgba(25, 145, 235,.5);
    border-radius: 4px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    cursor: pointer;
    -moz-transition: all .2s ease-in-out;
    -webkit-transition: all .2s ease-in-out;
    -ms-transition: all .2s ease-in-out;
    -o-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
}
.j-button:hover{
    background: rgba(25, 145, 235,1);
}

/* Customize select */
.j-select{
    position: relative;
}
.j-select-placeholder{
    padding: 0 25px 0 15px;
    height: 34px;
    font-size: 14px;
    line-height: 34px;
    color: #4fa0ef;
    background-color: rgba(6, 34, 71,0.5);
    border:1px solid rgb(55, 91, 148);
    border-radius: 4px;
    cursor: pointer;
}
.j-select-arrow{
    position: absolute;
    right: 10px;
    top: 12px;
    display: inline-block;
    width: 12px;
    height: 12px;
    background: url(../img/arrow_bottom.png) no-repeat;
    z-index: -1;
}
.j-select-arrow.active{
    background: url(../img/arrow_up.png) no-repeat;
}
.j-select-options{
    position: absolute;
    top: 50px;
    left: 0;
    z-index: 999;
    display: none;
    width: 100%;
    max-height: 155px;
    overflow-y: auto;
}
.j-select-options ul{
    padding: 2px;
    background-color: rgba(6,34,71,0.7);
    border-radius: 2px;
    color: #4fa0ef;
    border-radius: 5px;
}
.j-select-options li{
    display: block;
    padding: 8px 20px 8px 13px;
    font-size: 14px;
    line-height: 14px;
    cursor: pointer;
}
.j-select-options li:hover{
    background-color: #104983;
}

/* table */
.j-table{
    width: 100%;
    border:solid 1px rgba(21, 86, 146,.4);
    border-collapse: collapse;
    word-wrap: break-word;
    word-break: break-all;
}
.j-table th{
    height: 40px;
    font-size: 14px;
    font-weight: normal;
    color: #fff;
    background-color: rgba(25, 145, 235,.1);
    border:solid 1px rgba(21, 86, 146,.4);
    text-align: center;
}
.j-table td{
    padding: 13px 0;
    font-size: 14px;
    line-height: 1;
    color: #fff;
    border:solid 1px rgba(21, 86, 146,.4);
    text-align: center;
    text-indent: 10px;
}
.j-table .with-bg{
    background-color: rgba(25, 145, 235,.1);
}
.j-table-cell-140{
    width: 140px;
}

/* pop */
.pop{
    display: block;
}
.pop-header{
    position: relative;
    height: 40px;
    font-weight: normal;
    font-size: 16px;
    line-height: 40px;
    color: #fff;
    background: rgba(79,160,239,.8);
    text-align: center;
}
.close-pop-btn{
    position: absolute;
    right: 21px;
    top: 16px;
    display: inline-block;
    width: 10px;
    height: 10px;
    background: url(../img/close_pop.png) no-repeat;
    cursor: pointer;
}

/* �Զ���confirm */
.confirm{
}
.confirm-bg{
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5);
}
.confirm-modal{
    position: fixed;
    left: 50%;
    top: 50%;
    z-index: 1001;
    width: 300px;
    height: 150px;
    transform: translate(-50%,-50%);
    background: #101830;
    border: 1px solid #009efb;
    border-radius: 5px;
}
.confirm-info{
    padding: 0 20px;
    margin-top: 40px;
    font-size: 14px;
    color: #fff;
    text-align: center;
}
.confirm-options{
    position: absolute;
    bottom: 15px;
    left: 0;
    width: 100%;
    text-align: center;
}
.confirm-options .j-button{
    margin: 0 20px;
}
/* End components */

/* Common */
.j-container{
    min-width: 1200px;
    min-height: 100%;
}
/* background */
.website-bg {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: -1;
    height: 100%;
    overflow: hidden;
    opacity: 0;
    filter: alpha(opacity=0);
    -webkit-transition: opacity .8s ease;
    -moz-transition: opacity .8s ease;
    transition: opacity .8s ease;
}
.website-bg-show{
    opacity: 100;
    filter: alpha(opacity=100);
}

.text-box{
    padding: 0 25px 0 15px;
    width: 242px;
    height: 36px;
    line-height: 36px;
    color: #fff;
    background-color: rgba(6, 34, 71,0.5);
    border-radius: 4px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    word-break: break-all;
}

.form-item-err{
    font-size: 12px;
    color: #afd6ff;
}
/* End background */

/* header */
.header{
    height: 56px;
    font-size: 0;
    line-height: 56px;
    background-color: #1d3961;
}
.header-content{
    position: relative;
    margin: 0 47px;
}
.logo-name{
    font-size: 20px;
    color: #fff;
    vertical-align: middle;
}
.logo-name img{
    vertical-align: middle;
}
.logo-name:hover{
    color: #fff;
    text-decoration: none;
}
.header-logo{
    display: inline-block;
    height: 36px;
    width: 36px;
    margin-right: 10px;
    background-image: url(../img/mylogo_icon.png);
    background-repeat: no-repeat;
    background-position: left 7px;
    vertical-align: middle;
}
.login-name{
    float: right;
    max-width: 200px;
    margin-right: 25px;
    font-size: 14px;
    color: #fff;
    cursor: pointer;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.header-crtl{
    position: relative;
    float: right;
    margin-top: 23px;
    line-height: 1;
    overflow: visible;
}
.header-crtl-arrow{
    display: inline-block;
    width: 12px;
    height: 12px;
    background-image: url(../img/arrow_bottom.png);
    background-repeat: no-repeat;
    cursor: pointer;
}
.header-crtl-arrow.active{
    background-image: url(../img/arrow_up.png);
}
.header-crtl-list{
    position: absolute;
    top: 33px;
    right: 0;
    z-index: 999;
    display: none;
    width: 100px;
    padding: 2px;
    background: rgba(6,34,71,0.7);
    border-radius: 2px;
    color: #4fa0ef;
}
.header-crtl-list li{
    display: block;
    padding: 8px 20px 8px 13px;
    font-size: 14px;
    line-height: 14px;
    cursor: pointer;
}
.header-crtl-list li a{
    color: #3a3a3a;
    font-size: 14px;
}
.header-crtl-list li:hover{
    background-color: #104983;
}
.header-crtl-list li:hover a{
}
/* End header */

.content{
    min-height: 100%;
    margin-top: 30px;
    padding-bottom: 125px;
    overflow: hidden;
}

/* side-nav */
.side-nav{
    float: left;
    width: 264px;
    height: 600px;
    background-color: rgba(6, 34, 71,0.5);
}
.side-item{
    height: 48px;
    font-size: 0;
    line-height: 48px;
    overflow: hidden;
    cursor: pointer;
}
.side-item.selected{
    background: rgba(39, 155, 243,.1);
}
.highlight-bar{
    display: inline-block;
    float: left;
    width: 2px;
    height: 100%;
}
.selected .highlight-bar{
    background-color: #2b9ff7;
}
.side-nav-icon{
    float: left;
    display: inline-block;
    width: 21px;
    height: 21px;
    margin: 14px 14px 0 25px;
    background: url(../img/side_nav_icon.png);
    background-repeat: no-repeat;
}
.side-nav-icon1{
    background-position: 0 0;
}
.side-nav-icon2{
    background-position: 0 -20px;
}
.side-nav-icon3{
    background-position: 0 -40px;
}
.side-nav-icon4{
    background-position: 0 -60px;
}
.side-nav-icon5{
    background-position: 0 -80px;
}
.selected .side-nav-icon{
    background-image: url(../img/side_nav_icon_hover.png);
}
.side-nav-name{
    float: left;
    display: inline-block;
    width: 172px;
    font-size: 14px;
    color: #fff;
    vertical-align: middle;
}
.selected .side-nav-name{
    color: #2b9ff7;
}
.side-nav-arrow{
    display: inline-block;
    float: left;
    margin-top: 16px;
    width: 16px;
    height: 16px;
    background-image: url(../img/arrow_bottom.png);
    background-repeat: no-repeat;
}
.selected .side-nav-arrow{
    background-image: url(../img/arrow_up.png);
}
.side-sub-nav{
    display: none;
    margin-left: 61px;
    padding: 9px 0;
    line-height: 1;
}
.side-sub-nav.active{
    display: block;
}
.side-sub-nav li{
    padding: 9px 0;
    cursor: pointer;
}
.side-sub-nav li a{
    font-size: 14px;
    color: #fff;
    text-decoration: none;
}
.side-sub-nav li.selected,
.side-sub-nav li:hover{
    color: #009efb;
}
.side-sub-nav li.selected a,
.side-sub-nav li:hover a{
    color: #009efb;
}
/* End side-nav */

/* pagination */
.page_parent{
    margin-top: 30px;
    font-size: 0;
    text-align: right;
}
.page_list{
    display:inline-block;
    font-size: 0;
}
.page_list a,
.page_list strong{
    display: inline-block;
