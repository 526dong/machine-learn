    margin: 0 5px;
    font-size:12px;
    line-height: 1;
    color:#999;
    border: 1px solid rgb(55, 91, 148);
    border-radius: 2px;
    vertical-align: middle;
    cursor:pointer;
}
.page_list .active,
.page_list a:hover{
    border: 1px solid rgba(25, 145, 235,.8);
    background-color: rgba(25, 145, 235,.8);
    color: #fff;
}
.page_list span{
    margin: 0 5px;
    font-size:12px;
    line-height: 1;
    color:#6b6b6b;
    vertical-align: bottom;
}
.page_list strong{
    width: 12px;
    height: 12px;
}
.page_left{
    background:url(../img/page1.png) no-repeat;
    margin-right:8px;
    background-position: center center;
}
.page_right{
    background:url(../img/page2.png) no-repeat;
    margin-left:6px;
    background-position: center center;
}
.page_left:hover{
    border: 1px solid rgba(25, 145, 235,.8);
    background:url(../img/page4.png) no-repeat;
    background-position: center center;
}
.page_right:hover{
    border: 1px solid rgba(25, 145, 235,.8);
    background:url(../img/page3.png) no-repeat;
    background-position: center center;
}
/* End pagination */

.main{
    margin-left: 284px;
    margin-right: 30px;
}
.main-header{
    height: 50px;
}
.page-title{
    float: left;
}
.page-title h3{
    margin-bottom: 10px;
    line-height: 1;
    font-size: 24px;
    font-weight: normal;
    color: #11a0f8;
}
.page-title p{
    line-height: 1;
    font-size: 14px;
    color: #7684a6;
}
/* ��ҳ */
.index-nav{
    margin-top: 60px;
    font-size: 0;
    text-align: center;
}
.index-nav-item{
    margin: 20px 40px;
    display: inline-block;
}
.index-nav-icon{
