}
.import-file span{
    font-size: 14px;
    vertical-align: middle;
}
.import-file .add-file-icon{
    display: inline-block;
    width: 10px;
    height: 10px;
    margin: 0 12px 0 16px;
    background: url(../img/import_icon2.png) no-repeat;
