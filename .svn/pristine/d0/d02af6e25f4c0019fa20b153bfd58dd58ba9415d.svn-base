	<script type="text/javascript" src="${ctx}/resources/js/enterprise/ajaxfileupload.js"></script>
</head>
<script type="text/javascript">
/*页面初始化*/
$(function(){
	/*列表数据*/
	getData();
});
/*列表数据*/
function getData(){
    //数据文件id
    var dataFileId = ${dataFileId};

	$.ajax({
		url:"${ctx}/modelsDataFile/findFileInfo",
		type:'POST',
		data:{
			"dataFileId":dataFileId,
		},
		async: false,
		success:function(data){
			if (200 == data.code) {
			    alert("ok");
			} else {
                alert(data.msg);
                console.error(data.msg);
            }
		}
	});
}
